{
    "build": {
      "core": "sduino",
      "extra_flags": "-DSTM8S_DISCOVERY -DSTM8S003F --debug --verbose",
      "f_cpu": "16000000L",
      "cpu": "stm8",
      "mcu": "stm8s003f3p6",
      "mcu_stm8flash": "stm8s003f3p6",
      "variant": "icschallenge-board"
    },
    "debug": {
      "openocd_target": "stm8s",
      "tools": {
        "stlink": {
          "default": true,
          "server": {
            "arguments": [
              "-f",
              "scripts/interface/stlink.cfg",
              "-f",
              "scripts/target/stm8s003.cfg",
              "-c",
              "init",
              "-c", 
              "reset halt"
            ],
            "executable": "bin/openocd",
            "package": "tool-openocd"
          },
          "onboard": true
        }
      }
    },
    "frameworks": [
      "arduino",
      "spl"
    ],
    "upload": {
      "maximum_ram_size": 1024,
      "maximum_size": 8192,
      "protocol": "stlinkv2",
      "protocols": [
        "serial",
        "stlink",
        "stlinkv2"
      ]
    },
    "name": "I can solder SMD challenge board",
    "url": "hhtronik.com",
    "vendor": "HHTronik / Staudt Technologies GmbH"
  }
  